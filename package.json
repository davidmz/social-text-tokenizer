{
  "name": "social-text-tokenizer",
  "version": "2.3.1",
  "description": "Posts/comments text parser for FreeFeed.net",
  "homepage": "https://github.com/davidmz/social-text-tokenizer",
  "author": "David Mzareulyan <david@hiero.ru> (https://github.com/davidmz)",
  "license": "MIT",
  "main": "build/cjs/index.js",
  "module": "build/esm/index.js",
  "types": "build/esm/index.d.ts",
  "exports": {
    ".": {
      "import": "./build/esm/index.js",
      "default": "./build/cjs/index.js"
    },
    "./lib": {
      "import": "./build/esm/lib/index.js",
      "default": "./build/cjs/lib/index.js"
    }
  },
  "typesVersions": {
    "*": {
      "lib": [
        "./build/esm/lib/index.d.ts"
      ]
    }
  },
  "scripts": {
    "clean": "del-cli build/cjs build/esm",
    "build": "tsc && tsc -p ./tsconfig.esm.json",
    "test": "jest",
    "prepack": "run-s clean test build"
  },
  "devDependencies": {
    "@types/jest": "^29.4.0",
    "@types/lodash.escaperegexp": "^4.1.7",
    "@types/node": "^18.14.6",
    "@types/punycode": "~2.1.0",
    "del-cli": "^5.0.0",
    "jest": "^29.4.3",
    "npm-run-all": "~4.1.5",
    "prettier": "2.8.4",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "lodash.escaperegexp": "^4.1.2",
    "punycode": "^2.3.0",
    "tslib": "^2.5.0"
  }
}
